{% extends 'IngenieriaEstudianteBundle::layout.html.twig' %}

{% block programa %}
Aplicación WEB : [Estudiante]
{% endblock %}

{% block js  %}
<script>
$( "#actividades" ).css("background-color","#000");
$( "#actividades img" ).css("opacity","0.4");
$( "#actividades h3" ).css("opacity","0.4");
</script>

{% endblock %}

{% block content  %}
{{ parent() }}
<div class="content">
	{% block upSection  %}
	<div class = "upSection">
		<h1 class="sectionTitle">Inscripción Actividad complementaria</h1>
		<a href="javascript:history.back()" class = "button"><img src="{{ asset('img/regresar.png') }}"/><h4>Regresar</h4></a>
	</div>
	{% endblock  %}

	{% block downSection  %}
	<div class = "downSection"> 
		<div class="profileContainer">
		<h2 style="color : black;">Confirme su inscripcion en la actividad complementaria</h2>
		<a href="javascript:history.back()" class = "button"><img src="{{ asset('img/regresar.png') }}"/><h4>Confimar!</h4></a>
		</div>
	</div><!--downsection -->

	{% endblock %}
</div><!-- content -->
{% endblock %}
